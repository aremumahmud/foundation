      "destination": "/index.html"
    },
    {
      "source": "/programs",
      "destination": "/index.html"
    },
    {
      "source": "/our-works",
      "destination": "/index.html"
    },
    {
      "source": "/donate",
      "destination": "/index.html"
    },
    {
      "source": "/contact",
      "destination": "/index.html"
    }
  ]
}
